{
  "name": "hymnal",
  "version": "1.0.0",
  "description": "An offline-capable hymnal with search",
  "main": "index.html",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "rimraf docs/*",
    "buildModern": "babel src/js/hymns.js -o docs/js/hymns.js -presets=env",
    "buildLegacy": "babel --no-babelrc src/js/hymns.js > docs/js/hymns.legacy.js",
    "build": "echo '=> Building Source Files' && buildModern && buildLegacy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dons20/hymnal.git"
  },
  "keywords": [
    "hymnal"
  ],
  "author": "Keno Clayton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dons20/hymnal/issues"
  },
  "homepage": "https://github.com/dons20/hymnal#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "rimraf": "^2.6.2",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.4.8",
    "watch": "^1.0.2"
  }
}
